<?xml version="1.0" encoding="utf-8"?>
<cronjobs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:noNamespaceSchemaLocation="schemas/cronjob.xsd">
	<cronjob>
		<name>ITSW - Synchronize Google Merchant Center Product Feed</name>
		<action>Shopware_CronJob_ItswHandleGoogleMerchantCenterQueue</action>
		<active>true</active>
		<interval>120</interval>
		<disableOnError>false</disableOnError>
	</cronjob>
	
	<cronjob>
		<name>ITSW - Check Prepayment Orders Payment Status</name>
		<action>Shopware_CronJob_ItswCheckPrepaymentOrdersPaymentStatus</action>
		<active>true</active>
		<interval>86400</interval>
		<disableOnError>true</disableOnError>
	</cronjob>
</cronjobs>