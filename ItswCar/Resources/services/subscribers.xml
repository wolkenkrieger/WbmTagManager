<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
		   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	
    <parameters>
        <parameter key="itswcar.subscribers.class">ItswCar\Components\Subscribers\Subscribers</parameter>
    </parameters>

    <services>
        <service id="itswcar.subscribers" class="%itswcar.subscribers.class%">
            <argument type="service" id="service_container" />
            <argument type="service" id="itswcar.services" />
            <argument>%itsw_car.plugin_dir%</argument>
            <tag name="shopware.event_subscriber" />
        </service>
    </services>
</container>